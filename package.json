    "@ecomplus/auth": "^1.0.1",
    "@ecomplus/client": "^2.2.1",
    "@ecomplus/i18n": "^1.21.0",
    "@ecomplus/utils": "^1.4.1",
    "babel-runtime": "^6.26.0",
